<%- include("../partials/header.ejs") %>
<style>
    *{
        margin:0;
        padding:0;
        box-sizing: border-box;
    }
    :root{
        --primary: #5EAAA8;
        --secondary: #A3D2CA;
        --beige: #F7F3E9;
        --orange: #F05945;
    }
    body {
        max-height: 90vh;
        font-size: 16px;
        display: flex;
        flex-direction: row;
    }
    .image-container {
        height: 100vh;
        overflow: hidden;
        background-color: var(--secondary);
        width: 50vw;
    }
    .image-container img {
        height: 100vh;
        width: 50vw;
        object-fit: contain;
    }

    .form-container {
        width: 50vw;
        display: flex;
        justify-content: center;
        align-items: center;
    }
    .form-container form {
        border-radius: 10px;
        padding: 3vh 7vw;
        margin: 5vh 5vw;
        background-color: var(--beige);
        display: flex;
        flex-direction: column;
    }
    .form-container form label {
        color: var(--primary);
        font-size: 1.5rem;
    }
    .form-container form input {
        padding: 5px;
        margin-bottom: 7px;
        border: 1px solid black;
        border-radius: 5px;
    }
    .form-container form input:focus {
        outline: none;
    }
    .form-container form button {
        padding: 5px;
        margin-top: 2vh;
        border: 1px solid black;
        border-radius: 5px;
        background-color: var(--orange);
        color: var(--beige)
    }
    .form-container form a {
        margin-top: 3px;
        font-size: 0.9rem;
        color: var(--primary);
        text-decoration: none;
    }
</style>
<body>
    <div class="image-container">
        <img alt="filler project management image" src="data:image/jpeg;base64,">
    </div>
    <div class="form-container">
        <form method="POST" action="/register">

            <label for="username-field">User name</label>
            <input type="text" name="username" id="username-field" placeholder="Johnny Appleseed">
            
            <label for="email-field">Email</label>
            <input type="email" name="email" id="email-field" placeholder="Johnny@bigapples.com">

            <label for="password-field">Password</label>
            <input type="password" name="password" id="password-field">

            <button type="submit">Login</button>
            <a href="/login">Already have an account?</a>
        </form>
    </div>
</body>
</html>